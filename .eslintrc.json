{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "arrow-parens": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "spaced-comment": "off",
    "object-curly-newline": "off",
    "no-underscore-dangle": "off"
  }
}
